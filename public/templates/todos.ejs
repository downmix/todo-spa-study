<div id="todos">
  <% todos.forEach(todo => { %>
    <div class="todo-item" data-id="<%= todo.id %>">
      <input class="todo-checkbox" type="checkbox" <%= todo.complete ? "checked" : ""%>>
      <% if (todo.complete) { %>
        <strike><%= todo.title %></strike>
      <% } else { %>
        <%= todo.title %>
      <% } %>
      <% if (todo.label) { %>
        <span class="label"><%= todo.label %></span>
      <% } else { %>
        <span class="nolabel"></span>
      <% } %>
      <span class="time"><%= todo.time %></span>
      <i class="todo-remove fa fa-remove"></i>
    </div>
  <% }) %>  
  
</div>